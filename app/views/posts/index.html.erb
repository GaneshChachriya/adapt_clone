<section class="our-work-content">
  <div class="container">
    <h1 class="text-center animated fadeInUp" id= "workElimenth3">Blog Post</h1>
    <%= @posts.each_slice(3) do |posts| %>
      <div class="row">
        <%= render partial: "post", collection: posts %>
       </div>
     <% end %>
    <div class="pagination">
     <%= will_paginate @posts, renderer: BootstrapPagination::Rails%>
    </div>
  </div>
</section>
